cost <- (samples<=1)*(0.1 + 0.6*samples) + (samples>1)*(0.1+0.3*samples)
plot(samples, cost, xlab="Duración de la llamada", ylab = "Coste",
main = "Simulación del coste de 1000 llamadas")
mean(cost) # Media
sd(cost)   # Desviación típica
sum(cost)  # Coste total
